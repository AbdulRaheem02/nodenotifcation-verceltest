{
    "version":1,
    "crons": [
        {
          "path": "index.js", 
          "schedule": "* * * * *"
        }
      ],
    "builds":[
        {
        "src":"index.js",
        "use":"@now/node"
    }
],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"index.js"
        }
    ]
}