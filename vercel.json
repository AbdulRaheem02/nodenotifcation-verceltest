{
    "version":2,
    "crons": [
        {
          "path": "/api/cron",
          "schedule": "0 5 * * *"
        }
      ],
    "builds":[{
        "src":"index.js",
        "use":"@now/node"
    }],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"index.js"
        }
    ]
}