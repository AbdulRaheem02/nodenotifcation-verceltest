{
 
    "crons": [
        {
            "path": "/api",
            "schedule": "* * * * *"
          }
      ],
    "builds":[
        {
        "src":"index.js",
        "use":"@now/node"
    }
   ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"index.js"
        }
    ]
}
